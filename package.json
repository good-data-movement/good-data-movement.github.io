    "react-dom": "^16.8.6",
    "react-headroom": "^2.2.8",
    "react-helmet": "^5.2.1",
    "react-markdown": "^4.1.0",
    "react-reveal": "^1.2.2",
    "react-scroll-section": "^1.3.0",
    "react-text-loop": "^2.0.2",
    "react-tippy": "^1.2.3",
    "rebass": "^4.0.2"
